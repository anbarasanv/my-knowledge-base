    "editor.suggest.showSnippets": true,
    "editor.tabCompletion": "on",
    "workbench.colorCustomizations": {
        "activityBar.background": "#2A3101",
        "titleBar.activeBackground": "#3B4401",
        "titleBar.activeForeground": "#F7FECE"
    }
}